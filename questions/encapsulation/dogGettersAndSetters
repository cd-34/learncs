public class Dog {
    private String name;
    private double age;

    public Dog(String getName, double getAge) {
        assert getName != null && getAge >= 0;
        name = getName;
        age = getAge;
    }

    public double getAge() {
        assert age >= 0.0;
        return age;
    }

    public void setAge(double setAge) {
        assert setAge >= 0.0;
        age = setAge;
    }

    public String getName() {
        assert name != null;
        return name;
    }
}